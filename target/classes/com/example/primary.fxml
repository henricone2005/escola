<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="443.0" prefWidth="433.0" spacing="20.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.PrimaryController">
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
   <children>
      <VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="100.0" spacing="10.0">
         <children>
            <TextField fx:id="txtChave" promptText="Digite senha chave" />
            <Button mnemonicParsing="false" onAction="#verificarCredencial" prefHeight="25.0" prefWidth="69.0" text="proximo" />
            <TextField fx:id="txtSite" layoutX="10.0" layoutY="10.0" promptText="Qual site/platafoma :" />
            <TextField fx:id="txtSenha" layoutX="10.0" layoutY="35.0" promptText="Senha:" />
         </children>
      </VBox>
      <HBox alignment="CENTER" prefHeight="44.0" prefWidth="217.0" spacing="42.0">
         <children>
            <Button contentDisplay="CENTER" mnemonicParsing="false" onAction="#adicionarCredencial" prefHeight="25.0" prefWidth="60.0" text="salvar " textOverrun="CENTER_WORD_ELLIPSIS" />
         </children>
      </HBox>
      <ListView fx:id="listSenha" prefHeight="328.0" prefWidth="393.0" />
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="10.0" />
   </children>
</VBox>
